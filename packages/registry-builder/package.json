{
  "name": "@dotui/registry-builder",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts",
    "./cli": "./src/cli.ts",
    "./generators": "./src/generators/index.ts"
  },
  "bin": {
    "registry-builder": "./src/cli.ts"
  },
  "scripts": {
    "build": "tsx src/cli.ts --src ../registry/src --out ./dist",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@dotui/registry": "workspace:*",
    "@dotui/transformers": "workspace:*"
  },
  "devDependencies": {
    "@dotui/ts-config": "workspace:*",
    "@dotui/types": "workspace:*",
    "@types/node": "^22.15.3",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  }
}
