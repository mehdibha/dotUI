{
  "name": "@dotui/registry-definition",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./src/types.ts"
    },
    "./registry-ui": {
      "types": "./dist/registry-ui.d.ts",
      "default": "./src/registry-ui.ts"
    },
    "./registry-base": {
      "types": "./dist/registry-base.d.ts",
      "default": "./src/registry-base.ts"
    },
    "./registry-blocks": {
      "types": "./dist/registry-blocks.d.ts",
      "default": "./src/registry-blocks.ts"
    },
    "./registry-hooks": {
      "types": "./dist/registry-hooks.d.ts",
      "default": "./src/registry-hooks.ts"
    },
    "./registry-lib": {
      "types": "./dist/registry-lib.d.ts",
      "default": "./src/registry-lib.ts"
    },
    "./registry-icons": {
      "types": "./dist/registry-icons.d.ts",
      "default": "./src/registry-icons.ts"
    },
    "./registry-tokens": {
      "types": "./dist/registry-tokens.d.ts",
      "default": "./src/registry-tokens.ts"
    },
    "./registry-textures": {
      "types": "./dist/registry-textures.d.ts",
      "default": "./src/registry-textures.ts"
    },
    "./registry-shadows": {
      "types": "./dist/registry-shadows.d.ts",
      "default": "./src/registry-shadows.ts"
    },
    "./registry-bg-patterns": {
      "types": "./dist/registry-bg-patterns.d.ts",
      "default": "./src/registry-bg-patterns.ts"
    },
    "./registry-fonts": {
      "types": "./dist/registry-fonts.d.ts",
      "default": "./src/registry-fonts.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@dotui/eslint-config": "workspace:*",
    "@dotui/prettier-config": "workspace:*",
    "@dotui/ts-config": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "shadcn": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@dotui/prettier-config"
}
