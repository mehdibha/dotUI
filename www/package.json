    "@react-stately/utils": "^3.10.1",
    "@remixicon/react": "^4.2.0",
    "@types/mdx": "^2.0.13",
    "@vercel/analytics": "^1.1.1",
    "babel-plugin-react-compiler": "19.0.0-beta-63e3235-20250105",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "fumadocs-core": "^14.0.2",
    "fumadocs-docgen": "^1.3.0",
    "fumadocs-mdx": "^11.1.0",
    "hast-util-to-string": "^3.0.1",
    "immer": "^10.1.1",
    "jotai": "2.8.0",
    "jotai-immer": "^0.4.1",
    "lucide-react": "^0.469.0",
    "motion": "^11.16.4",
    "nanoid": "^5.0.7",
    "next": "15.1.4",
    "next-themes": "^0.2.1",
