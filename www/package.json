{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:registry": "tsx ./scripts/build-registry.ts && prettier --log-level silent --write \"src/registry/**/*.{ts,tsx,mdx}\" \"src/__demos__/**/*.{ts,tsx}\" --cache",
    "build:registry-demos": "tsx ./scripts/build-registry-demos.ts && prettier --log-level silent --write \"src/__demos__/registry.ts\" --cache",
    "build:demos": "tsx ./scripts/build-demos.ts && prettier --log-level silent --write \"src/__demos__/index.tsx\" --cache",
    "start": "next start",
    "dev": "next dev --turbo",
    "preview": "next build && next start",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@adobe/leonardo-contrast-colors": "^1.0.0",
    "@content-collections/mdx": "^0.1.6",
    "@fumadocs/content-collections": "^1.1.4",
    "@internationalized/date": "^3.5.4",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-use-controllable-state": "^1.0.1",
    "@react-aria/toolbar": "3.0.0-beta.4",
    "@react-aria/utils": "^3.24.0",
    "@react-stately/utils": "^3.10.1",
    "@remixicon/react": "^4.2.0",
    "@vercel/analytics": "^1.1.1",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "fumadocs-core": "^13.4.10",
    "geist": "^1.2.2",
    "lucide-react": "^0.311.0",
    "next": "14.2.14",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "react": "18.3.1",
    "react-aria": "^3.33.0",
    "react-aria-components": "^1.2.1",
    "react-dom": "18.3.1",
    "react-resizable-panels": "^2.0.19",
    "react-stately": "^3.31.0",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.2",
    "shiki": "^1.6.2",
    "tailwind-merge": "^2.2.0",
    "tailwind-variants": "^0.2.1",
    "tsx": "^4.7.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@content-collections/core": "^0.7.2",
    "@content-collections/next": "^0.2.3",
    "@dotui/eslint-config": "workspace:*",
    "@dotui/registry": "workspace:*",
    "@dotui/ts-config": "workspace:*",
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.31",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^1.1.2",
    "typescript": "^5"
  }
}
