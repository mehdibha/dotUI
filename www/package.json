{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:registry": "tsx ./scripts/build-registry.ts && prettier --log-level silent --write \"src/registry/**/*.{ts,tsx,mdx}\" \"src/__demos__/**/*.{ts,tsx}\" --cache",
    "build:internal-registry": "tsx ./scripts/build-internal-registry.ts && prettier --log-level silent --write \"src/__registry__/**/*.{ts,tsx,mdx}\" --cache",
    "build:demos": "tsx ./scripts/build-demos.ts && prettier --log-level silent --write \"src/__demos__/index.tsx\" --cache",
    "start": "next start",
    "dev": "next dev --turbopack",
    "preview": "next build && next start",
    "format": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache --log-level warn",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache --log-level warn",
    "typecheck": "tsc --noEmit",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "postinstall": "fumadocs-mdx"
  },
